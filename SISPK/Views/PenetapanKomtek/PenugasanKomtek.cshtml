<script type="text/javascript">
    function cekjenis(ini) {
        var res = parseInt($(ini).val());
        if (res == 2 || res == 3) {
            $(".revisi_list").slideDown("fast", function () {
                getsni();
            });
        } else {
            $(".revisi_list").slideUp("fast", function () {
                removesni();
            });
        }
    }
    function set_jalur(ini) {
        if ($(ini).is(':checked')) {
            $(".adopsi_list").slideDown("fast", function () {

            });
        } else {
            $(".adopsi_list").slideUp("fast", function () {

            });
        }
        var Jalur1 = $('#PROPOSAL_JALUR_1').is(':checked');
        var Jalur2 = $('#PROPOSAL_JALUR_2').is(':checked');
        if (Jalur1 == true && Jalur2 == true) {
            $('#PROPOSAL_JALUR').val(3);
        } else if (Jalur1 == true && Jalur2 == false) {
            $('#PROPOSAL_JALUR').val(1);
        } else if (Jalur1 == false && Jalur2 == true) {
            $('#PROPOSAL_JALUR').val(2);
        }
    }
    function cekmetode(ini) {
        var res = parseInt($(ini).val());
        if (res == 3) {
            $(".mod_ind_list").slideDown("fast", function () {

            });
        } else {
            $(".mod_ind_list").slideUp("fast", function () {

            });
        }
    }

    function set_adop_modif() {
        var value = $('input[name="PROPOSAL_IS_ADOPSI_MOD"]:checked').val();
        if (value == 1) {
            $('.adopsimodifikasinomor').prop("readonly", false);
        } else {
            $('.adopsimodifikasinomor').val('');
            $('.PROPOSAL_NO_STANDAR_MODIFIKASI').val('');
            $('.adopsimodifikasinomor').prop("readonly", true);

        }
    }
    function sethakpaten() {
        var value = $('input[name="PROPOSAL_IS_HAK_PATEN"]:checked').val();
        if (value == 0) {
            $('#PROPOSAL_IS_HAK_PATEN_DESC').prop("readonly", true);
        } else {
            $('#PROPOSAL_IS_HAK_PATEN_DESC').prop("readonly", false);
            $('#PROPOSAL_IS_HAK_PATEN_DESC').val('');
        }
    }
    function send_no(ini) {
        var nomorstandar = $('#nomorstandaradopsi').val();
        var tahunstandar = $('#tahunstandaradopsi').val();
        var judulstandar = $('#judulstandaradopsi').val();
        var res = nomorstandar + ((tahunstandar == "") ? "" : ":" + tahunstandar) + "," + judulstandar;
        $('#PROPOSAL_NO_STD_ADOPSI_IDENTIK').val(res);
    }
    function send_no_mod(ini) {
        var nomorstandar = $('#nomorstandarmodifikasi').val();
        var tahunstandar = $('#tahunstandarmodifikasi').val();
        var judulstandar = $('#judulstandarmodifikasi').val();
        var res = nomorstandar + ((tahunstandar == "") ? "" : ":" + tahunstandar) + "," + judulstandar;
        $('#PROPOSAL_NO_STANDAR_MODIFIKASI').val(res);
    }
    function removesni() {
        $("#PROPOSAL_REVISI_SNI_ID").select2().val(null).trigger("change");
        $("#PROPOSAL_TERJEMAHAN_SNI_ID").select2().val(null).trigger("change");
    }
    function getsni() {

        $("#PROPOSAL_REVISI_SNI_ID").select2({
            allowClear: true,
            width: '100%',
            ajax: {
                url: '/Pengajuan/Usulan/FindSNI',
                dataType: 'json',
                delay: 250,
                data: function (params) {
                    return {
                        q: params.term, // search term
                        page: params.page
                    };
                },
                processResults: function (data, params) {
                    params.page = params.page || 1;
                    return {
                        results: data.sni,
                        pagination: {
                            more: (params.page * 10) < data.total_count
                        }
                    };
                },
                cache: true
            },
            minimumInputLength: 2,
        });
        $("#PROPOSAL_TERJEMAHAN_SNI_ID").select2({
            allowClear: true,
            width: '100%',
            ajax: {
                url: '/Pengajuan/Usulan/FindSNI',
                dataType: 'json',
                delay: 250,
                data: function (params) {
                    return {
                        q: params.term, // search term
                        page: params.page
                    };
                },
                processResults: function (data, params) {
                    params.page = params.page || 1;
                    return {
                        results: data.sni,
                        pagination: {
                            more: (params.page * 10) < data.total_count
                        }
                    };
                },
                cache: true
            },
            minimumInputLength: 2,
        });
    }
    function find_ics(ini) {
        var res = $(ini).val();
        var KOMTEK_ID = $('#PROPOSAL_KOMTEK_ID').val();
        if (KOMTEK_ID != "") {
            $('.dtkomtek').slideDown('fast', function () {
                $('.dtkomtek').attr('onclick', 'detail_komtek(' + KOMTEK_ID + ')');
            });
        } else {
            $('.dtkomtek').slideUp('fast', function () {
                $('.dtkomtek').removeAttr('onclick');
            });
        }
        $("#PROPOSAL_ICS_REF_ICS_ID").select2('destroy');
        $("#PROPOSAL_ICS_REF_ICS_ID").val('');
        var formData = { KOMTEK_ID: res };
        $.ajax({
            type: 'POST',
            url: "/Pengajuan/PenetapanKomtek/FindICS",
            contentType: "application/json",
            dataType: 'json',
            data: JSON.stringify(formData)
        }).done(function (data) {
            var ics = data.ics;
            var opsi = "<option value=''></option>";
            if (ics != null) {
                for (var i = 0; i < ics.length; i++) {
                    opsi += "<option value='" + ics[i].KOMTEK_ICS_ICS_ID + "'>" + ics[i].ICS_CODE + " " + ((ics[i].ICS_NAME_IND == null) ? ics[i].ICS_NAME : ics[i].ICS_NAME_IND) + "</option>";
                }
                $('#PROPOSAL_ICS_REF_ICS_ID').html(opsi);
                $("#PROPOSAL_ICS_REF_ICS_ID").select2({
                    allowClear: false,
                    width: '100%'
                });
            }
            //$.when(tutuploading()).then(window.location = "/Pengajuan/Usulan");
        });
    }

    function simpankomtek() {
        bootbox.dialog({
            message: "Anda yakin menyimpan Surat Penugasan Komite Teknis ini ?",
            title: "Konfirmasi Pembuatan Surat Penugasan Komite Teknis",
            buttons: {
                success: {
                    name: "Yakin",
                    id: "Yakin",
                    label: "Yakin",
                    className: "green",
                    callback: function () {
                        bukaloading();
                        var validator = $("#sispk_form").validate();
                        var valid = validator.form();
                        if (valid == true) {

                            var PROPOSAL_ID = $('#PROPOSAL_ID').val();
                            var PROPOSAL_ST_KOM_NO = $('#PROPOSAL_ST_KOM_NO').val();
                            var PROPOSAL_ST_KOM_LAMPIRAN = $('#PROPOSAL_ST_KOM_LAMPIRAN').val();
                            var PROPOSAL_ST_KOM_DATE = $('#PROPOSAL_ST_KOM_DATE').val();
                            var PROPOSAL_ST_KOM_TIME = $('#PROPOSAL_ST_KOM_TIME').val();

                            var formData = { PROPOSAL_ID: PROPOSAL_ID, PROPOSAL_ST_KOM_NO: PROPOSAL_ST_KOM_NO, PROPOSAL_ST_KOM_LAMPIRAN: PROPOSAL_ST_KOM_LAMPIRAN, PROPOSAL_ST_KOM_DATE: PROPOSAL_ST_KOM_DATE, PROPOSAL_ST_KOM_TIME: PROPOSAL_ST_KOM_TIME };
                            $.ajax({
                                type: 'POST',
                                url: "/Pengajuan/PenetapanKomtek/PenugasanKomtek",
                                contentType: "application/json",
                                dataType: 'json',
                                data: JSON.stringify(formData)
                            }).done(function (data) {
                                $.when(tutuploading()).then(window.location = "/Pengajuan/PenetapanKomtek");
                            });
                        } else {
                            tutuploading()
                        }
                    }
                },
                danger: {
                    name: "Yakin",
                    id: "Yakin",
                    label: "Batal",
                    className: "red",
                    callback: function () {

                    }
                }
            }
        });
    }
    jQuery(document).ready(function () {
        $('.timepicker-no-seconds').timepicker({
            autoclose: true,
            minuteStep: 5,
            showMeridian: false
        });
        $('.date-picker').datepicker({
            orientation: "left",
            autoclose: true
        });
    });
</script>
