@{
    var Detail = ViewBag.DataProposal;
}
<style>
    .select2-selection--single {
        height: 100% !important;
    }

    .select2-selection__rendered {
        word-wrap: break-word !important;
        text-overflow: inherit !important;
        white-space: normal !important;
    }
</style>
<div class="row">
    <div class="col-md-12">
        <!-- BEGIN PORTLET-->
        <div class="portlet box green">
            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-gift"></i>Form Data Penetapan Komtek
                </div>
            </div>
            <div class="portlet-body form">
                <!-- BEGIN FORM-->
                <form class="form-horizontal form-bordered" id="sispk_form" action="" method="post" enctype="multipart/form-data">
                    <div class="form-body">
                        <div class="form-group">
                            <label class="control-label col-md-3">Kode PNPS</label>
                            <div class="col-md-9 control-label detailtext">
                                <a href="javascript:void(0)" style="color: blue; text-decoration: none;" onclick="$('#detail').modal('show')">@Detail.PROPOSAL_PNPS_CODE</a>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-3">Komite Teknis / Subkomite Teknis</label>
                            <div class="col-md-7 control-label detailtext">
                                <a href="javascript:void(0)" onclick="detail_komtek(@Detail.KOMTEK_ID)" style="color: blue; text-decoration: none;">@Detail.KOMTEK_CODE @Detail.KOMTEK_NAME</a>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-3">Judul PNPS</label>
                            <div class="col-md-9 control-label detailtext">
                                @Detail.PROPOSAL_JUDUL_PNPS
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-3">Ruang Lingkup</label>
                            <div class="col-md-9 control-label detailtext">
                                @Detail.PROPOSAL_RUANG_LINGKUP
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-3">No Surat Penugasan<span class="required">*</span></label>
                        <div class="col-md-4">
                            <div class="input-icon right">
                                <input type="text" id="PROPOSAL_ST_KOM_NO" name="PROPOSAL_ST_KOM_NO" class="form-control wajib">
                                <input type="hidden" id="PROPOSAL_ID" name="PROPOSAL_ID" class="form-control" value="@Detail.PROPOSAL_ID">
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-3">Lampiran Surat Penugasan<span class="required">*</span></label>
                        <div class="col-md-4">
                            <div class="input-icon right">
                                <input type="text" id="PROPOSAL_ST_KOM_LAMPIRAN" name="PROPOSAL_ST_KOM_LAMPIRAN" class="form-control wajib">
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-3">Tanggal Rapat Teknis<span class="required">*</span></label>
                        <div class="col-md-4">
                            <div class="input-group input-medium date date-picker" data-date-format="yyyy-mm-dd" data-date-start-date="+0d">
                                <input type="text" class="form-control wajib" id="PROPOSAL_ST_KOM_DATE" name="PROPOSAL_ST_KOM_DATE">
                                <span class="input-group-btn">
                                    <a class="btn default" type="button"><i class="fa fa-calendar"></i></a>
                                </span>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-3">Waktu Rapat Teknis<span class="required">*</span></label>

                        <div class="col-md-4">
                            <div class="input-group input-medium">
                                <input type="text" id="PROPOSAL_ST_KOM_TIME" name="PROPOSAL_ST_KOM_TIME" class="form-control timepicker timepicker-no-seconds wajib">
                                <span class="input-group-btn">
                                    <a class="btn default" type="button"><i class="fa fa-clock-o"></i></a>
                                </span>
                            </div>
                        </div>
                    </div>
                    <div class="form-actions">
                        <div class="row">
                            <div class="col-md-offset-3 col-md-9">
                                <a class="btn green " href="javascript:void(0)" onclick="simpankomtek()">Simpan</a> <a class="btn blue " href="@Url.Action("Index")">Kembali</a>
                            </div>
                        </div>
                    </div>
                </form>
                <!-- END FORM-->
            </div>
        </div>
        <!-- END PORTLET-->
    </div>
</div>






<!--Detail DATA-->
<div class="modal fade bs-modal-lg" id="detail" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                <h4 class="modal-title">Detail Usulan @Detail.PROPOSAL_JUDUL_PNPS</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <!-- BEGIN PORTLET-->
                        <div class="portlet box green">
                            <div class="portlet-title">
                                <div class="caption">
                                    <i class="fa fa-gift"></i>Detail Data Pengajuan Usulan
                                </div>
                            </div>
                            <div class="portlet-body form">
                                <!-- BEGIN FORM-->
                                <div class="portlet-body form">
                                    <!-- BEGIN FORM-->
                                    <div class="form-horizontal form-bordered">
                                        <div class="form-body">
                                            @if (Detail.PROPOSAL_TYPE == 1)
                                            {
                                                <div class="form-group">
                                                    <label class="control-label col-md-3">Komite Teknis / Subkomite Teknis <span class="required">*</span></label>
                                                    <div class="col-md-9 control-label detailtext">
                                                        @Detail.KOMTEK_CODE. @Detail.KOMTEK_NAME
                                                    </div>
                                                </div>
                                                if (ViewBag.ICS.Count > 0)
                                                {
                                                <div class="form-group">
                                                    <label class="control-label col-md-3">ICS</label>
                                                    <div class="col-md-9" style="padding-top: 20px">
                                                        <div class="detailtext">
                                                            <ol style="padding-left: 15px;">
                                                                @foreach (var an in ViewBag.ICS)
                                                                {
                                                                    <li>@an.ICS_CODE @((an.ICS_NAME_IND == null) ? an.ICS_NAME : an.ICS_NAME_IND)</li>
                                                                }
                                                            </ol>
                                                        </div>
                                                    </div>
                                                </div>
                                                }
                                                <div class="form-group">
                                                    <label class="control-label col-md-3">Konseptor <span class="required">*</span></label>
                                                    <div class="col-md-9 control-label detailtext">
                                                        @Detail.PROPOSAL_KONSEPTOR
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label col-md-3">Institusi Konseptor <span class="required">*</span></label>
                                                    <div class="col-md-9 control-label detailtext">
                                                        @Detail.PROPOSAL_INSTITUSI
                                                    </div>
                                                </div>
                                            }
                                            else
                                            {
                                                <div class="form-group">
                                                    <label class="control-label col-md-3">Nama <span class="required">*</span></label>
                                                    <div class="col-md-9 control-label detailtext">
                                                        @Detail.PROPOSAL_TIM_NAMA
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label col-md-3">Institusi <span class="required">*</span></label>
                                                    <div class="col-md-9 control-label detailtext">
                                                        @Detail.PROPOSAL_INSTITUSI
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label col-md-3">Nama <span class="required">*</span></label>
                                                    <div class="col-md-9 control-label detailtext">
                                                        @Detail.PROPOSAL_TIM_ALAMAT
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label col-md-3">Nama <span class="required">*</span></label>
                                                    <div class="col-md-9 control-label detailtext">
                                                        @Detail.PROPOSAL_TIM_PHONE
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label col-md-3">Nama <span class="required">*</span></label>
                                                    <div class="col-md-9 control-label detailtext">
                                                        @Detail.PROPOSAL_TIM_EMAIL
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label col-md-3">Nama <span class="required">*</span></label>
                                                    <div class="col-md-9 control-label detailtext">
                                                        @Detail.PROPOSAL_TIM_FAX
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label col-md-3">Komite Teknis / Subkomite Teknis <span class="required">*</span></label>
                                                    <div class="col-md-9 control-label detailtext">
                                                        @((Detail.PROPOSAL_KOMTEK_ID == null) ? "Komtek Belum Ditetapkan" : Detail.KOMTEK_CODE + ". " + Detail.KOMTEK_NAME)
                                                    </div>
                                                </div>
                                                if (ViewBag.ICS.Count > 0)
                                                {
                                                <div class="form-group">
                                                    <label class="control-label col-md-3">ICS</label>
                                                    <div class="col-md-9" style="padding-top: 20px">
                                                        <div class="detailtext">
                                                            <ol style="padding-left: 15px;">
                                                                @foreach (var an in ViewBag.ICS)
                                                                {
                                                                    <li>@an.ICS_CODE @((an.ICS_NAME_IND == null) ? an.ICS_NAME : an.ICS_NAME_IND)</li>
                                                                }
                                                            </ol>
                                                        </div>
                                                    </div>
                                                </div>
                                                }
                                            }
                                            <div class="form-group">
                                                <label class="control-label col-md-3">Judul PNPS <span class="required">*</span></label>
                                                <div class="col-md-9 control-label detailtext">
                                                    @Detail.PROPOSAL_JUDUL_PNPS
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3">Ruang Lingkup <span class="required">*</span></label>
                                                <div class="col-md-9 control-label detailtext">
                                                    @Detail.PROPOSAL_RUANG_LINGKUP
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3">Jenis Perumusan SNI <span class="required">*</span></label>
                                                <div class="col-md-9 control-label detailtext">
                                                    @Detail.PROPOSAL_JENIS_PERUMUSAN_NAME
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3">Jalur Perumusan SNI Baru / Revisi <span class="required">*</span></label>
                                                <div class="col-md-9">
                                                    <div style="text-align: left" class="input-group col-md-12">
                                                        <div class="checkbox-list">
                                                            <label>
                                                                <input type="checkbox" value="1" name="PROPOSAL_JALUR_TEMP" id="PROPOSAL_JALUR_1" class="" @((Detail.PROPOSAL_JALUR == 1 || Detail.PROPOSAL_JALUR == 3) ? "checked" : "") disabled="disabled">
                                                                Perumusan sendiri berdasarkan penelitian
                                                            </label>
                                                            <label>
                                                                <input type="checkbox" value="2" name="PROPOSAL_JALUR_TEMP" id="PROPOSAL_JALUR_2" class="" @((Detail.PROPOSAL_JALUR == 2 || Detail.PROPOSAL_JALUR == 3) ? "checked" : "") disabled="disabled">
                                                                Adopsi identik standar atau publikasi internasional
                                                            </label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3">Metode Adopsi</label>
                                                @if (Detail.PROPOSAL_METODE_ADOPSI == 1)
                                                { 
                                                    <div class="col-md-9 control-label detailtext">Publikasi ulang - cetak ulang</div>
                                                }
                                                else if (Detail.PROPOSAL_METODE_ADOPSI == 2)
                                                {
                                                    <div class="col-md-9 control-label detailtext">Terjemahan dua bahasa (Indonesia dan Inggris)</div>
                                                }
                                                else if (Detail.PROPOSAL_METODE_ADOPSI == 3)
                                                {
                                                    <div class="col-md-9 control-label detailtext">Terjemahan satu bahasa (Indonesia)</div>
                                                }
                                                else
                                                {
                                                    <div class="col-md-9 control-label detailtext">-</div>
                                                }
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3">Standar ISO/IEC/SDO lain yang akan diadopsi secara <b>Identik</b></label>
                                                <div class="col-md-9" style="padding-top: 20px">
                                                    <div class="detailtext">
                                                        @Detail.PROPOSAL_NO_STD_ADOPSI_IDENTIK
                                                    </div>
                                                    <div class="help-block">
                                                        Sebutkan standar ISO/EIC/SDO lain (Misalnya ASTM) yang akan diadopsi.(beri nomor standar, tahun terbitan dan judul standar yang akan diadopsi secara identik.)
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3">Nomor Judul SNI yang akan diterjemahkan</label>
                                                <div class="col-md-9" style="padding-top: 20px">
                                                    @if (Detail.PROPOSAL_TERJEMAHAN_SNI_ID != null)
                                                    {
                                                        <div class="detailtext">
                                                            @Detail.PROPOSAL_TERJEMAHAN_NOMOR @Detail.PROPOSAL_TERJEMAHAN_JUDUL
                                                        </div>
                                                    }
                                                    else
                                                    {
                                                        <div class="detailtext">-</div>
                                                    }
                                                    <div class="help-block">
                                                        Apabila usulan ini merupakan terjemahan dari SNI rep-rep yang sudah dipublikasikan. Maka sebutkan nomor dan judul SNI yang akan diterjemahkan.
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3">Apakah Adopsi Modifikasi standar atau publikasi internasional</label>
                                                <div class="col-md-9" style="padding-top: 20px">
                                                    <div style="text-align: left" class="input-group col-md-12">
                                                        <div class="checkbox-list">
                                                            <label>
                                                                <input type="checkbox" value="1" name="PROPOSAL_IS_ADOPSI_MOD" id="PROPOSAL_IS_ADOPSI_MOD_1" class="" @((Detail.PROPOSAL_IS_ADOPSI_MOD == 1) ? "checked" : "") disabled="disabled">
                                                                Ya
                                                            </label>
                                                            <label>
                                                                <input type="checkbox" value="2" name="PROPOSAL_IS_ADOPSI_MOD" id="PROPOSAL_IS_ADOPSI_MOD_2" class="" @((Detail.PROPOSAL_IS_ADOPSI_MOD == 2) ? "checked" : "") disabled="disabled">
                                                                Tidak
                                                            </label>
                                                        </div>
                                                    </div>
                                                    <div class="help-block">
                                                        Adopsi modifikasi standar atau publikasi internasional (ISO/IEC/SDO lain (misalnya ASTM)) (hanya untuk metode terjemahan dalam bahasa indonesia saja).
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="control-label col-md-3">Sebutkan standar ISO/IEC/SDO lain yang akan diadopsi secara <b>Modifikasi</b></label>
                                                <div class="col-md-9" style="padding-top: 20px">
                                                    @if (Detail.PROPOSAL_NO_STANDAR_MODIFIKASI != null)
                                                    {
                                                        <div class="detailtext">
                                                            @Detail.PROPOSAL_NO_STANDAR_MODIFIKASI
                                                        </div>
                                                    }
                                                    else
                                                    {
                                                        <div class="detailtext">-</div>
                                                    }
                                                    <div class="help-block">
                                                        Sebutkan standar ISO/EIC/SDO lain (Misalnya ASTM) yang akan diadopsi.(beri nomor standar, tahun terbitan dan judul standar yang akan diadopsi secara Modifikasi).)
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3">Kebutuhan Mendesak ?</label>
                                                <div class="col-md-9" style="padding-top: 20px">
                                                    <div class="detailtext">
                                                        @Detail.PROPOSAL_IS_URGENT_NAME
                                                    </div>
                                                    <div class="help-block">
                                                        Kebutuhan mendesak untuk regulasi teknis (disertai dengan surat dari pejabat setingkat Eselon 1 di instansi teknis yang relevan dengan kebutuhan SNI).
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3">Nomor Judul SNI yang akan di Amandemen/Ralat/Revisi</label>
                                                <div class="col-md-9" style="padding-top: 20px">
                                                    @if (Detail.PROPOSAL_REVISI_SNI_ID != null)
                                                    {
                                                        <div class="detailtext">
                                                            @Detail.PROPOSAL_REVISI_NOMOR @Detail.PROPOSAL_REVISI_JUDUL
                                                        </div>
                                                    }
                                                    else
                                                    {
                                                        <div class="detailtext">-</div>
                                                    }
                                                    <div class="help-block">
                                                        Apabila usulan ini merupakan amandemen/ralat/revisi maka sebutkan nomor dan judul SNI yang akan diusulkan.
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3">Pasal SNI yang di Amandemen/Ralat/Revisi</label>
                                                <div class="col-md-9" style="padding-top: 20px">
                                                    @if (Detail.PROPOSAL_REVISI_PASAL != null)
                                                    {
                                                        <div class="detailtext">
                                                            @Detail.PROPOSAL_REVISI_PASAL
                                                        </div>
                                                    }
                                                    else
                                                    {
                                                        <div class="detailtext">-</div>
                                                    }
                                                    <div class="help-block">
                                                        Sebutkan bagian atau pasal dari SNI yang akan dilakukan Amandemen/Ralat/Revisi.
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3">Terdapat isi dari standar yang terkait dengan hak paten ?</label>
                                                <div class="col-md-9" style="padding-top: 20px">
                                                    <div style="text-align: left" class="input-group col-md-12">
                                                        <div class="checkbox-list">
                                                            <label>
                                                                <input type="checkbox" value="1" name="PROPOSAL_IS_HAK_PATEN" id="PROPOSAL_IS_HAK_PATEN_1" class="" @((Detail.PROPOSAL_IS_HAK_PATEN == 1) ? "checked" : "") disabled="disabled">
                                                                Ya
                                                            </label>
                                                            <label>
                                                                <input type="checkbox" value="2" name="PROPOSAL_IS_HAK_PATEN" id="PROPOSAL_IS_HAK_PATEN_2" class="" @((Detail.PROPOSAL_IS_HAK_PATEN == 0) ? "checked" : "") disabled="disabled">
                                                                Tidak
                                                            </label>
                                                        </div>
                                                    </div>
                                                    <div class="help-block">
                                                        Apakah terdapat isi dari standar yang terkait dengan hak paten ?
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3">Informasi Hak Paten</label>
                                                <div class="col-md-9 control-label detailtext">
                                                    @Detail.PROPOSAL_IS_HAK_PATEN_DESC
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3">Informasi detail hasil penelitian atau kajian terhadap SNI</label>
                                                <div class="col-md-9" style="padding-top: 20px">
                                                    @if (Detail.PROPOSAL_INFORMASI != null)
                                                    {
                                                        <div class="detailtext">
                                                            @Detail.PROPOSAL_INFORMASI
                                                        </div>
                                                    }
                                                    else
                                                    {
                                                        <div class="detailtext">-</div>
                                                    }
                                                    <div class="help-block">
                                                        Berikan informasi detail, termasuk hasil penelitian atau kajian terhadap penerapan SNI tersebut, Jika perlu, tulis pada lembaran terpisah sebagai lampiran
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3">Informasi detail hasil penelitian atau kajian terhadap SNI</label>
                                                <div class="col-md-9">
                                                    <div class="help-block" style="font-size: 14px !important; color: #333333;">
                                                        @if (Detail.PROPOSAL_LAMPIRAN_FILE_PATH != "" && Detail.PROPOSAL_LAMPIRAN_FILE_PATH != null)
                                                        { 
                                                            <div class="help-block">
                                                                <a href="~@Detail.PROPOSAL_LAMPIRAN_FILE_PATH" target="_blank" style="color:green;">Download Informasi Detail Hasil Penelitian</a>
                                                            </div>   
                                                        }
                                                        else
                                                        {
                                                            <div class="help-block">
                                                                -
                                                            </div>   
                                                        }
                                                    </div>

                                                    <div class="help-block">
                                                        Lampiran Terpisah Informasi detail hasil penelitian atau kajian terhadap SNI
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3">Tujuan dan alasan spesifik mengenai perumusan yang akan dilakukan</label>
                                                <div class="col-md-9" style="padding-top: 20px">
                                                    @if (Detail.PROPOSAL_TUJUAN != null)
                                                    {
                                                        <div class="detailtext">
                                                            @Detail.PROPOSAL_TUJUAN
                                                        </div>
                                                    }
                                                    else
                                                    {
                                                        <div class="detailtext">-</div>
                                                    }
                                                    <div class="help-block">
                                                        Tujuan dan alasan spesifik mengenai perumusan yang akan dilakukan (termasuk alasan dilakukan amandemen/revisi/ralat untuk SNI tersebut.
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3">Kesesuaian dengan program pemerintah (sebutkan secara terperinci)</label>
                                                <div class="col-md-9" style="padding-top: 20px">
                                                    @if (Detail.PROPOSAL_PROGRAM_PEMERINTAH != null)
                                                    {
                                                        <div class="detailtext">
                                                            @Detail.PROPOSAL_PROGRAM_PEMERINTAH
                                                        </div>
                                                    }
                                                    else
                                                    {
                                                        <div class="detailtext">-</div>
                                                    }
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3">Pihak-pihak utama yang Berkepentingan</label>
                                                <div class="col-md-9" style="padding-top: 20px">
                                                    @if (Detail.PROPOSAL_PIHAK_BERKEPENTINGAN != null)
                                                    {
                                                        <div class="detailtext">
                                                            @Html.Raw(Detail.PROPOSAL_PIHAK_BERKEPENTINGAN.Replace("|", "<br>"))
                                                        </div>
                                                    }
                                                    else
                                                    {
                                                        <div class="detailtext">-</div>
                                                    }
                                                    <div class="help-block">
                                                        Pihak-pihak utama yang berkepentingan yang dapat mengambil manfaat dari kegiatan perumusan
                                                    </div>
